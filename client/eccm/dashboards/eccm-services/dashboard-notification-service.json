            "editorMode": "code",
            "exemplar": true,
            "expr": "rate(notificationservice_send_telegram_notification_metric_total{host=\"$host\", instance=\"$instance\"}[$precision])",
            "hide": false,
            "legendFormat": "{{reason}}",
