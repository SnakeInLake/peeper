          {
            "datasource": {
              "type": "prometheus",
              "uid": "${prometheus}"
            },
            "editorMode": "code",
            "exemplar": true,
            "expr": "rate(notificationservice_send_mail_notification_metric_total{host=\"$host\", instance=\"$instance\"}[$precision])",
            "hide": false,
            "legendFormat": "{{reason}}",
            "range": true,
