              "uid": "${prometheus}"
            },
            "editorMode": "code",
            "exemplar": true,
            "expr": "rate(notificationservice_send_websocket_notification_metric_total{host=\"$host\", instance=\"$instance\"}[$precision])",
            "hide": false,
            "legendFormat": "{{reason}}",
            "range": true,
            "refId": "C"
          }
        ],
        "title": "Rate на отправку уведомлений",
        "type": "timeseries"
